    },
    "rules": {
      "prefer-object-spread/prefer-object-spread": "off",
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "trailingComma": "es5"
        }
      ]
    }
  },
  "dependencies": {
    "@yarnpkg/lockfile": "^1.0.0",
    "semver": "^6.0.0"
  }
