{
  "name": "package-installed-version-sync",
  "version": "1.0.5",
  "description": "Synchronously returns installed version of a dependency package",
  "main": "index.js",
  "scripts": {
    "test": "jest"
  },
  "engines": {
    "node": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tinovyatkin/package-installed-version-sync.git"
  },
  "keywords": [
    "npm",
    "package",
    "version",
    "sync"
  ],
  "author": "Tino Vyatkin <tino@vtkn.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tinovyatkin/package-installed-version-sync/issues"
  },
  "homepage": "https://github.com/tinovyatkin/package-installed-version-sync#readme",
  "devDependencies": {
    "@destinationstransfers/eslint-config": "^1.0.2",
    "eslint": "^4.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "jest": "^21.2.1",
    "remark-cli": "^4.0.0",
    "remark-preset-lint-consistent": "^2.0.1",
    "remark-preset-lint-recommended": "^3.0.1"
  },
  "remarkConfig": {
    "presets": [
      "lint-recommended",
      "lint-consistent"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "testEnvironment": "node",
    "verbose": true,
    "bail": true
  },
  "eslintConfig": {
    "plugins": [
      "node"
    ],
    "extends": [
      "@destinationstransfers",
      "plugin:node/recommended"
    ],
    "env": {
      "node": true
    },
    "parserOptions": {
      "sourceType": "script"
    },
    "rules": {
      "prefer-object-spread/prefer-object-spread": "off",
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "trailingComma": "es5"
        }
      ]
    }
  }
}
