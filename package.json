{
  "name": "package-installed-version-sync",
  "version": "2.0.3",
  "description": "Synchronously returns installed version of a dependency package",
  "main": "index.js",
  "scripts": {
    "test": "jest"
  },
  "engines": {
    "node": ">=7"
  },
  "files": [
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tinovyatkin/package-installed-version-sync.git"
  },
  "keywords": [
    "npm",
    "package",
    "version",
    "sync"
  ],
  "author": "Tino Vyatkin <tino@vtkn.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tinovyatkin/package-installed-version-sync/issues"
  },
  "homepage": "https://github.com/tinovyatkin/package-installed-version-sync#readme",
  "devDependencies": {
    "@destinationstransfers/eslint-config": "6.4.2",
    "browser-logos": "github:alrra/browser-logos#55.0.1",
    "eslint": "5.16.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-node": "5.2.1",
    "jest": "24.8.0",
    "remark-cli": "6.0.1",
    "remark-preset-lint-consistent": "2.0.1",
    "remark-preset-lint-recommended": "3.0.1"
  },
  "remarkConfig": {
    "presets": [
      "lint-recommended",
      "lint-consistent"
    ]
